<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.softwarepatternsca2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SoftwarePatternsCA2">
        <activity android:name=".CustomerFeatures.SelectedStockItem"></activity>
        <activity android:name=".CustomerFeatures.BrowseItems" />
        <activity android:name=".AdminFeatures.AddStockItem" />
        <activity android:name=".AdminFeatures.WelcomeAdmin" />
        <activity android:name=".CustomerFeatures.WelcomeCustomer" />
        <activity android:name=".RegisterLogin.RegisterCustomer" />
        <activity android:name=".RegisterLogin.AdminLogin" />
        <activity android:name=".RegisterLogin.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>